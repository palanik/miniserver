{
  "name": "miniserver",
  "version": "1.0.0",
  "description": "Mini http server, serving static files",
  "main": "lib/index.js",
  "bin": {
    "serve": "./bin/miniserver"
  },
  "preferGlobal": true,
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb/base"
  },
  "scripts": {
    "lint": "eslint src/**",
    "build": "babel --presets es2015 src -d lib",
    "prepublish": "npm run build",
    "start": "node lib/",
    "dev": "./node_modules/.bin/babel-node src/"
  },
  "engines": {
    "node": ">=6.1.0",
    "npm": ">=3.8.6"
  },
  "homepage": "https://github.com/palanik/miniserver",
  "repository": {
    "type": "git",
    "url": "https://github.com/palanik/miniserver.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "N. Palani Kumanan",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.13.0"
  },
  "dependencies": {
    "basic-auth": "^1.0.4",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "finalhandler": "^0.5.0",
    "morgan": "^1.7.0",
    "serve-index": "^1.8.0",
    "serve-static": "^1.11.1",
    "yargs": "^5.0.0"
  }
}
